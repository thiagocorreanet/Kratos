@model QueryGenerateCodeGetByIdResponse

@{
    ViewData["Title"] = "Códigos API";
}

<div class="page-header d-print-none">
    <div class="container-xl">
        <div class="row g-2 align-items-center">
            <div class="col">
                <!-- Page pre-title -->
                <div class="page-pretitle">
                    <a href="/">Dashboard</a>
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="20" viewBox="0 0 24 20" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="icon icon-tabler icons-tabler-outline icon-tabler-arrow-narrow-right">
                        <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                        <path d="M5 12l14 0" />
                        <path d="M15 16l4 -4" />
                        <path d="M15 8l4 4" />
                    </svg>
                    <a href="#">Gerar API</a>
                </div>
                <h2 class="page-title">
                    Gerar estrutura API
                </h2>
            </div>
        </div>
    </div>
</div>

<div class="page-body">
    <div class="container-xl">
        <div class="row row-cards">
            <div class="col-12">
                <!-- Formulário Principal -->
                <form method="post" class="card">
                    <input type="hidden" id="Id" value="123" />
                    <!-- Tabs para Navegação -->
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <ul class="nav nav-tabs card-header-tabs" data-bs-toggle="tabs">
                                    <li class="nav-item">
                                        <a href="#registration-data" class="nav-link active" data-bs-toggle="tab">
                                            &nbsp; Core
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="#audit" class="nav-link" data-bs-toggle="tab">
                                            &nbsp; Infrastructure
                                        </a>
                                    </li>

                                    <li class="nav-item">
                                        <a href="#audit" class="nav-link" data-bs-toggle="tab">
                                            &nbsp; Application
                                        </a>
                                    </li>

                                    <li class="nav-item">
                                        <a href="#audit" class="nav-link" data-bs-toggle="tab">
                                            &nbsp; API
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div class="card-body">
                                <div class="tab-content">
                                    <div class="tab-pane active show" id="registration-data">
                                        <div class="accordion" id="accordion-example">
                                            <div class="accordion-item">
                                                <h2 class="accordion-header" id="heading-1">
                                                    <button class="accordion-button " type="button" data-bs-toggle="collapse" data-bs-target="#collapse-1" aria-expanded="true">
                                                        Entities
                                                    </button>
                                                </h2>
                                                <div id="collapse-1" class="accordion-collapse collapse show" data-bs-parent="#accordion-example">
                                                    <div class="accordion-body pt-0">
                                                        <div class="card-code">
                                                            <figure class="highlight">
                                                               <pre><code class="language-csharp">
@Model.Entities


    </code></pre>
                                                            </figure>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="accordion-item">
                                                <h2 class="accordion-header" id="heading-2">
                                                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapse-2" aria-expanded="false">
                                                        Abstract
                                                    </button>
                                                </h2>
                                                <div id="collapse-2" class="accordion-collapse collapse" data-bs-parent="#accordion-example">
                                                    <div class="accordion-body pt-0">
                                                        <div class="card-code">
                                                            <figure class="highlight">
                                                               <pre><code class="language-csharp">
@Model.Abstract


    </code></pre>
                                                            </figure>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="tab-pane" id="audit">
                                        <div class="row row-cards">
                                            <div class="col-sm-6 col-md-6 col-lg-6">
                                                <div class="mb-3">
                                                    <label class="form-label" >Data de criação</label>
                                                    <input type="text" class="form-control"  disabled>
                                                </div>
                                            </div>

                                            <div class="col-sm-6 col-md-6 col-lg-6">
                                                <div class="mb-3">
                                                    <label class="form-label" >Data de alteração</label>
                                                    <input type="text" class="form-control"  disabled>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>

                                <hr />

                                <div>
                                    <div class="d-flex justify-content-between">
                                        <a href="/lista-de-projetos" class="btn btn-secondary">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="icon icon-tabler icons-tabler-outline icon-tabler-corner-down-left">
                                                <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                                <path d="M18 6v6a3 3 0 0 1 -3 3h-10l4 -4m0 8l-4 -4" />
                                            </svg>
                                            Voltar
                                        </a>

                                        <div class="d-flex gap-2">
                                            <a href="#" class="btn btn-danger" data-bs-toggle="modal" data-bs-target="#modal-delete">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="icon icon-tabler icons-tabler-outline icon-tabler-trash">
                                                    <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                                    <path d="M4 7l16 0" />
                                                    <path d="M10 11l0 6" />
                                                    <path d="M14 11l0 6" />
                                                    <path d="M5 7l1 12a2 2 0 0 0 2 2h8a2 2 0 0 0 2 -2l1 -12" />
                                                    <path d="M9 7v-3a1 1 0 0 1 1 -1h4a1 1 0 0 1 1 1v3" />
                                                </svg>
                                                Excluir
                                            </a>
                                            <a href="#" class="btn btn-primary" onclick="putData()">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="icon icon-tabler icons-tabler-outline icon-tabler-device-floppy">
                                                    <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                                    <path d="M6 4h10l4 4v10a2 2 0 0 1 -2 2h-12a2 2 0 0 1 -2 -2v-12a2 2 0 0 1 2 -2" />
                                                    <path d="M12 14m-2 0a2 2 0 1 0 4 0a2 2 0 1 0 -4 0" />
                                                    <path d="M14 4l0 4l-6 0l0 -4" />
                                                </svg>
                                                Salvar
                                            </a>
                                        </div>
                                    </div>
                                </div>



                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>